{
    "manifest_version": 3,
    "name": "Focus Booster 2.0",
    "version": "2.0",
    "description": "Stay focused and productive as a customer support agent.",
    "permissions": [
      "storage",
      "alarms",
      "notifications"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }
